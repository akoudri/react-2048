{
    "family": "Nginx-Task",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "923692348898.dkr.ecr.eu-west-3.amazonaws.com/jenkins-training:APP_VERSION",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "nginx-port",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}