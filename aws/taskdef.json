{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "JenkinsTraining-TaskDef-Prod",
    "containerDefinitions": [
        {
            "name": "jenkins-training",
            "image": "923692348898.dkr.ecr.eu-west-3.amazonaws.com/jenkins-training:APP_VERSION",
            "portMappings": [
                {
                    "name": "nginx-port-mapping",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::923692348898:role/ecsTaskExecutionRole"
}